{
  "manifest_version": 2,
  "name": "mFill",
  "version": "1.0",
  "description": "Automatically fills out web forms, uploads specified files, and other fun stuff.",
  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/logo-32.png",
    "default_title": "mFill",
    "default_popup": "mFill/menu.html"
  }
}